{
  "engines": {
    "node": "^8.10.0"
  },
  "name": "code-challenge",
  "version": "1.0.0",
  "description": "parent node config for code-challenge project. This orchestrates interaction betweenh the client and server sides of the project.",
  "main": "index.js",
  "repository": "https://mbifulco@github.com/gymnasium/code-challenge.git",
  "author": "Mike Bifulco <mbifulco@aquent.com>",
  "license": "MIT",
  "scripts": {
    "code-challenge-client": "cd client && yarn start",
    "code-challenge-server": "cd server && yarn start",
    "dev-client": "cd client && yarn start",
    "dev-server": "cd server && yarn start:dev",

    "start": "concurrently 'npm:code-challenge-*'",
    "start:dev": "concurrently 'npm:dev-*'"
  },
  "dependencies": {
    "concurrently": "^4.0.1"
  }
}
